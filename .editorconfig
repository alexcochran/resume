# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

max_line_length = 120

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
charset = utf-8

# 2 space indentation
[*.{json,ts,js}]
indent_style = space
indent_size = 2
